    Title = "ğŸ”„ é‡ç½®äººç‰©",
    Description = "å°†è§’è‰²ä¼ é€åˆ°è™šç©º",
    Callback = function()
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            player.Character.HumanoidRootPart.CFrame = CFrame.new(0, -975, 0)
            WindUI:Notify({
                Title = "é‡ç½®äººç‰©",
                Content = "å·²å°†è§’è‰²ä¼ é€åˆ°è™šç©º",
                Icon = "refresh-cw",
                Duration = 3
            })
        else
            WindUI:Notify({
                Title = "é”™è¯¯",
                Content = "æ— æ³•æ‰¾åˆ°è§’è‰²æˆ–HumanoidRootPart",
                Icon = "x",
                Duration = 3
            })
        end
    end
})

-- æ— åŒè§’è‰²
