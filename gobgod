-- 8. 新增的自动功能标签页
local autoTab = Window:Tab({
    Title = "自动功能",
    Icon = "rotate-ccw",
    Locked = false,
})

-- 自动抽奖功能
autoTab:Toggle({
    Title = "自动抽奖",
    Description = "自动进行抽奖活动",
    Callback = function(value)
        if value then
            WindUI:Notify({
                Title = "自动抽奖",
                Content = "已开启自动抽奖功能",
                Duration = 3
            })
            -- 这里可以添加自动抽奖的逻辑
        else
            WindUI:Notify({
                Title = "自动抽奖",
                Content = "已关闭自动抽奖功能",
                Duration = 3
            })
        end
    end
})

-- 自动购买物品功能
autoTab:Section({
    Title = "自动购买物品",
    Description = "选择要自动购买的物品"
