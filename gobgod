-- 加载 WindUI 库
local success, WindUI = pcall(function()
    return loadstring(game:HttpGet("https://raw.githubusercontent.com/Syndromehsh/Lua/refs/heads/main/AlienX/AlienX%20Wind%203.0%20UI.txt"))()
end)

-- 检查 WindUI 是否加载成功
if not success or not WindUI then
    warn("WindUI 加载失败，请检查网络连接或链接有效性")
    return
end

task.wait(2)

WindUI:Notify({
    Title = "freestbb",
    Content = "stbb",
    Duration = 4
})

task.wait(0.5)

local player = game.Players.LocalPlayer

local Window = WindUI:CreateWindow({
    Title = "gobstbb<font color='#00FF00'>2.0</font> / stbb",
    Icon = "rbxassetid://4483362748",
    IconTransparency = 1,
    Author = "stbb",
    Folder = "stbb",
    Size = UDim2.fromOffset(100, 150),
    Transparent = true,
    Theme = "Dark",
    UserEnabled = true,
    SideBarWidth = 200,
    HasOutline = true,
    User = {
        Enabled = true,
        Anonymous = false,
        Username = player.Name,
        DisplayName = player.DisplayName,
        UserId = player.UserId,
        Thumbnail = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. player.UserId .. "&width=420&height=420&format=png",
        Callback = function()
            WindUI:Notify({
                Title = "用户信息",
                Content = "玩家: " .. player.Name .. " (" .. player.DisplayName .. ")",
                Duration = 3
            })
        end
    }
})

task.wait(0.3)

Window:EditOpenButton({
    Title = "身体棒棒",
    Icon = "monitor",
    CornerRadius = UDim.new(0,16),
    StrokeThickness = 2,
    Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromHex("FF0000")),
        ColorSequenceKeypoint.new(0.16, Color3.fromHex("FF7F00")),
        ColorSequenceKeypoint.new(0.33, Color3.fromHex("FFFF00")),
        ColorSequenceKeypoint.new(0.5, Color3.fromHex("00FF00")),
        ColorSequenceKeypoint.new(0.66, Color3.fromHex("0000FF")),
        ColorSequenceKeypoint.new(0.83, Color3.fromHex("4B0082")),
        ColorSequenceKeypoint.new(1, Color3.fromHex("9400D3"))
    }),
    Draggable = true,
})

task.wait(0.2)

local Tab = Window:Tab({
    Title = "角色控制",
    Icon = "user",
    Locked = false,
})

-- 1. 重置人物 - 传送角色到虚空
Tab:Button({
    Title = "重置人物",
    Description = "将角色传送到虚空",
    Callback = function()
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            player.Character.HumanoidRootPart.CFrame = CFrame.new(0, -975, 0)
            WindUI:Notify({
                Title = "重置人物",
                Content = "已将角色传送到虚空",
                Duration = 3
            })
        else
            WindUI:Notify({
                Title = "错误",
                Content = "无法找到角色或HumanoidRootPart",
                Duration = 3
            })
        end
    end
})

-- 2. 传送到原点
Tab:Button({
    Title = "传送到原点",
    Description = "将角色传送到地图中心",
    Callback = function()
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            player.Character.HumanoidRootPart.CFrame = CFrame.new(0, 100, 0)
            WindUI:Notify({
                Title = "传送",
                Content = "已传送到地图中心",
                Duration = 3
            })
        end
    end
})

-- 3. 预设速度按钮（替代滑块）
local speedTab = Window:Tab({
    Title = "移动速度",
    Icon = "gauge",
    Locked = false,
})

-- 预设速度选项
local speedOptions = {
    {Title = "慢速 (16)", Value = 16},
    {Title = "正常 (25)", Value = 25},
    {Title = "快速 (50)", Value = 50},
    {Title = "极速 (100)", Value = 100},
    {Title = "超速 (200)", Value = 200}
}

for _, option in ipairs(speedOptions) do
    speedTab:Button({
        Title = option.Title,
        Description = "设置移动速度为 " .. option.Value,
        Callback = function()
            if player.Character and player.Character:FindFirstChild("Humanoid") then
                player.Character.Humanoid.WalkSpeed = option.Value
                WindUI:Notify({
                    Title = "移动速度",
                    Content = "已设置为: " .. option.Value,
                    Duration = 2
                })
            end
        end
    })
end

-- 4. 跳跃高度设置
local jumpTab = Window:Tab({
    Title = "跳跃高度",
    Icon = "arrow-up",
    Locked = false,
})

local jumpOptions = {
    {Title = "正常跳跃 (50)", Value = 50},
    {Title = "高跳 (100)", Value = 100},
    {Title = "超高跳 (200)", Value = 200},
    {Title = "超级跳 (500)", Value = 500}
}

for _, option in ipairs(jumpOptions) do
    jumpTab:Button({
        Title = option.Title,
        Description = "设置跳跃高度为 " .. option.Value,
        Callback = function()
            if player.Character and player.Character:FindFirstChild("Humanoid") then
                player.Character.Humanoid.JumpPower = option.Value
                WindUI:Notify({
                    Title = "跳跃高度",
                    Content = "已设置为: " .. option.Value,
                    Duration = 2
                })
            end
        end
    })
end

-- 5. 无敌模式开关
local utilityTab = Window:Tab({
    Title = "实用功能",
    Icon = "shield",
    Locked = false,
})

utilityTab:Toggle({
    Title = "无敌模式",
    Description = "开启/关闭无敌模式",
    Callback = function(value)
        if value then
            -- 这里可以添加无敌模式的实现
            WindUI:Notify({
                Title = "无敌模式",
                Content = "已开启无敌模式",
                Duration = 3
            })
        else
            WindUI:Notify({
                Title = "无敌模式",
                Content = "已关闭无敌模式",
                Duration = 3
            })
        end
    end
})

-- 6. 飞行功能
utilityTab:Toggle({
    Title = "飞行模式",
    Description = "开启/关闭飞行能力",
    Callback = function(value)
        if value then
            WindUI:Notify({
                Title = "飞行模式",
                Content = "已开启飞行模式",
                Duration = 3
            })
        else
            WindUI:Notify({
                Title = "飞行模式",
                Content = "已关闭飞行模式",
                Duration = 3
            })
        end
    end
})

